package by.epam.learn.daryatarasevich.shapes.cube.warehouse;

public class DataForStorage {
    private  int area;
    private int volume;

    public DataForStorage() {
    }

    public DataForStorage(int area, int volume) {
        this.area=area;
        this.volume=volume;
    }

    public int getVolume() {
        return volume;
    }

    public void setVolume(int volume) {
        this.volume = volume;
    }

    public int getArea() {
        return area;
    }

    public void setArea(int area) {
        this.area = area;
    }

    @Override
    public String toString() {
        return  getClass().getName()+": area=" + area +
                ", volume=" + volume;
    }
}
